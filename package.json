{
  "name": "dasp-project",
  "version": "1.0.0",
  "description": "DASP Project with SCSS architecture",
  "main": "index.html",
  "scripts": {
    "build-css": "sass scss/main.scss public/css/main.css --style=compressed --source-map",
    "watch-css": "sass scss/main.scss public/css/main.css --watch --style=expanded --source-map",
    "dev": "sass scss/main.scss public/css/main.css --watch --style=expanded --source-map",
    "build": "npm run build-css && mkdir -p public && cp -r css public/ 2>/dev/null || true && cp -r img public/ 2>/dev/null || true && cp -r fonts public/ 2>/dev/null || true && cp -r favicon public/ 2>/dev/null || true && cp -r js public/ 2>/dev/null || true && cp *.html public/ 2>/dev/null || true",
    "clean": "rm -rf public/css/main.css public/css/main.css.map"
  },
  "devDependencies": {
    "sass": "^1.69.0"
  },
  "dependencies": {
    "swiper": "^12.0.1"
  },
  "keywords": ["scss", "css", "frontend", "vercel", "telegram", "form"],
  "author": "",
  "license": "MIT"
}
